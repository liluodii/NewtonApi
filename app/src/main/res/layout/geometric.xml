<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newton110"
    tools:context=".Second_fragment">

    <Spinner
        android:id="@+id/spinner_trigonometri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="210dp"
        android:background="@android:drawable/btn_dropdown"
        android:spinnerMode="dropdown"/>
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="310dp"
        android:text="Input"
        android:textColor="#F2000000"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/geoExpression"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="300dp"
        android:ems="10"
        android:hint="Enter your Expression"
        android:inputType="textPersonName"
        android:textColor="#110B0B" />

    <Button
        android:id="@+id/btngeoresult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="400dp"
        android:background="#F8E6CCCC"
        android:text="Get Your Result" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="130dp"
        android:background="#FFFCFC"
        android:textColor="#5E0606"
        android:text="Trigonometry Expression"
        android:textSize="30dp" />


</FrameLayout>